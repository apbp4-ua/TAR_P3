    </model>

    <model name="column_5">
      <static>1</static>
      <link name="link">
        <pose>3 -3 1.0 0 0 0</pose>
        <collision name="collision">
          <geometry><cylinder><radius>0.3</radius><length>2</length></cylinder></geometry>
        </collision>
        <visual name="visual">
          <geometry><cylinder><radius>0.3</radius><length>2</length></cylinder></geometry>
